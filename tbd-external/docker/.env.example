# Environment variables for Docker Compose (DEV and PROD)
# Copy to .env and set your values. Both dev.docker-compose.yml and prod.docker-compose.yml use these.

# Database (Liberty app + Oracle container)
DB_URL=jdbc:oracle:thin:@oracle-db:1521:ORCL
DB_USERNAME=tbd_user
DB_PASSWORD=your_secure_password_here
DB_POOL_SIZE=10

# Oracle container root password
ORACLE_PASSWORD=OraclePass123
